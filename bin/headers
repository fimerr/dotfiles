#!/usr/bin/env zsh
#
### print headers received from server
###
### https://github.com/rtomayko/dotfiles/blob/rtomayko/.local/bin/headers
if [[ $1 =~ ("-h"|"--help") ]]; then
    source print-comment "$(realpath $0)" $1
    exit 0
fi

curl -sv "$@" 2>&1 >/dev/null |
    grep -v "^\*" |
    grep -v "^}" |
    cut -c3-
